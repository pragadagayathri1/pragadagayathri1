def add(a,b):
    print(a+b)
def sub(a,b):
    print(a-b)
def multi(a,b):
    print(a*b)
def div(a,b):
    print(a%b)
a=int(input('Enter a number:'))
b=int(input('Enter next number:'))
print('1:addition,2:subtraction,3:multiplication,4:division')
c=int(input('Enter a choice:'))
if c==1:
   add(a,b)
elif c==2:
     sub(a,b)
elif c==3:
     multi(a,b)
elif c==4:
     div(a,b)
else:
     print('Wrong choice')
